FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

  application       pisoFoamTurbine;

  startFrom         startTime;

  startTime         1050.0;

  stopAt            endTime;

  endTime           1800.0;

  deltaT            0.0333333333333333333333333333333;

  writeControl      adjustableRunTime;

  writeInterval     150;

  purgeWrite        0;

  writeFormat       binary;

  writePrecision    12;

  writeCompression  compressed;

  timeFormat        general;

  timePrecision     12;

  runTimeModifiable yes;

  adjustTimeStep    no;

  maxCo             0.75;

  maxDeltaT         25.0;

  startTimeMean    330;

  startTimeCorr    660;

  functions
  {

  }


