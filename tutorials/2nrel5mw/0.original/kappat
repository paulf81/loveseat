FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      kappat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include        "include/initialConditions"

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform $kappat0;

boundaryField
{
    lower
    {
        type            fixedValue;
        value           uniform 0.0;
    }
    upper
    {
        type            zeroGradient;
        value           $internalField;
    }

    west
    {
        type            zeroGradient;
        value           $internalField;
    }
    east
    {
        type            zeroGradient;
        value           $internalField;
    }

    north 
    {
        type            cyclic;
        value           $internalField;
    }
    south
    {
        type            cyclic;
        value           $internalField;
    }
}


// ************************************************************************* //
